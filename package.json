{
  "name": "after",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "dev:gen": "bun openapi && react-router dev",
    "build": "bun openapi:prod && react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "check": "biome check && bun rr:check",
    "write": "bun biome:format && bun biome:lint",
    "biome:format": "biome format --write .",
    "biome:lint": "biome lint --write .",
    "rr:check": "react-router typegen && tsc --noEmit",
    "openapi": "bun openapi:hono && bun openapi:auth",
    "openapi:hono": "openapi-typescript http://localhost:3000/openapi.json --output app/schema.d.ts",
    "openapi:auth": "openapi-typescript openapi/better-auth.json --output app/schema-auth.d.ts",
    "openapi:prod": "bun openapi:prod:hono && bun openapi:prod:auth",
    "openapi:prod:hono": "openapi-typescript https://claciecookies-api.oktavianusrtasak.com/openapi.json --output app/schema.d.ts",
    "openapi:prod:auth": "openapi-typescript openapi/better-auth.json --output app/schema-auth.d.ts"
  },
  "dependencies": {
    "@better-fetch/fetch": "^1.1.18",
    "@conform-to/react": "^1.6.1",
    "@conform-to/zod": "^1.6.1",
    "@fontsource/dancing-script": "^5.2.5",
    "@mapbox/mapbox-gl-geocoder": "^5.1.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@types/mapbox-gl": "^3.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.10.5",
    "isbot": "^5.1.27",
    "lucide-react": "^0.511.0",
    "mapbox-gl": "^3.15.0",
    "openapi-fetch": "^0.14.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-map-gl": "^8.0.4",
    "react-router": "^7.5.3",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "openapi-typescript": "^7.8.0",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
